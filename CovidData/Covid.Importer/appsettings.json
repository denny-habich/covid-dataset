{
  "CovidDatabaseSettings": {
    "CovidDataCollectionName": "CovidData",
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "CovidDb"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ImportOptions": {
    "ImportIntervalSeconds": "1800",
    "MarginBetweenManualImportsSeconds": "30",
    "Source": "..\\Data",
    "FilePattern": "*.csv",
    "FieldsSeparator":  "|" 
  },
  "AllowedHosts": "*"
}
